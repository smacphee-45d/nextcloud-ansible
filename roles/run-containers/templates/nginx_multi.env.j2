SERVER_NAME={{ server_names }}
MULTISITE={{ multisite }}
{{ self_signed_ip }}_GENERATE_SELF_SIGNED_SSL=yes
{{ domain_ip }}_AUTO_LETS_ENCRYPT=yes
REDIRECT_HTTP_TO_HTTPS=yes
DISABLE_DEFAULT_SERVER=yes
MAX_CLIENT_SIZE=10G
HTTP2=no
USE_CLIENT_CACHE=yes
REMOTE_PHP=nextcloud
REMOTE_PHP_PATH=/var/www/html
LIMIT_REQ_RATE=5r/s
LIMIT_REQ_BURST=10
ALLOWED_METHODS=GET|POST|HEAD|PROPFIND|DELETE|PUT|MKCOL|MOVE|COPY|PROPPATCH|REPORT|UNLOCK|OPTIONS
X_FRAME_OPTIONS=SAMEORIGIN
USE_GZIP=yes
USE_BAD_BEHAVIOR=no
WHITELIST_USER_AGENT=WebDAV iOS